// Low-density value line devices are STM32F100xx microcontrollers where the Flash memory density
// ranges between 16 and 32 Kbytes.
#if defined(LMCU_STM32F100C4) || defined(LMCU_STM32F100R4) || defined(LMCU_STM32F100C6) || \
    defined(LMCU_STM32F100R6)
  #define LMCU_STM32F1_LOW_DENSITY_VALUE_LINE
#endif

// Medium-density value line devices are STM32F100xx microcontrollers where the Flash memory density
// ranges between 64 and 128 Kbytes.
#if defined(LMCU_STM32F100C8) || defined(LMCU_STM32F100R8) || defined(LMCU_STM32F100V8) || \
    defined(LMCU_STM32F100CB) || defined(LMCU_STM32F100RB) || defined(LMCU_STM32F100VB)
  #define LMCU_STM32F1_MEDIUM_DENSITY_VALUE_LINE
#endif

// High-density value line devices are STM32F100xx microcontrollers where the Flash memory density
// ranges between 256 and 512 Kbytes.
#if defined(LMCU_STM32F100RC) || defined(LMCU_STM32F100VC) || defined(LMCU_STM32F100ZC) || \
    defined(LMCU_STM32F100RD) || defined(LMCU_STM32F100VD) || defined(LMCU_STM32F100ZD) || \
    defined(LMCU_STM32F100RE) || defined(LMCU_STM32F100VE) || defined(LMCU_STM32F100ZE)
  #define LMCU_STM32F1_HIGH_DENSITY_VALUE_LINE
#endif

// Low-density devices are STM32F101xx, STM32F102xx and STM32F103xx microcontrollers where the
// Flash memory density ranges between 16 and 32 Kbytes.
#if defined(LMCU_STM32F101R4) || defined(LMCU_STM32F101T4) || defined(LMCU_STM32F101C6) || \
    defined(LMCU_STM32F101R6) || defined(LMCU_STM32F101T6) || defined(LMCU_STM32F102C4) || \
    defined(LMCU_STM32F102R4) || defined(LMCU_STM32F102C6) || defined(LMCU_STM32F102R6) || \
    defined(LMCU_STM32F103C4) || defined(LMCU_STM32F103R4) || defined(LMCU_STM32F103T4) || \
    defined(LMCU_STM32F103C6) || defined(LMCU_STM32F103R6) || defined(LMCU_STM32F103T6)
  #define LMCU_STM32F1_LOW_DENSITY
#endif

// Medium-density devices are STM32F101xx, STM32F102xx and STM32F103xx microcontrollers where the
// Flash memory density ranges between 64 and 128 Kbytes.
#if defined(LMCU_STM32F101C8) || defined(LMCU_STM32F101R8) || defined(LMCU_STM32F101T8) || \
    defined(LMCU_STM32F101V8) || defined(LMCU_STM32F101CB) || defined(LMCU_STM32F101RB) || \
    defined(LMCU_STM32F101TB) || defined(LMCU_STM32F101VB) || defined(LMCU_STM32F102C8) || \
    defined(LMCU_STM32F102R8) || defined(LMCU_STM32F102CB) || defined(LMCU_STM32F102RB) || \
    defined(LMCU_STM32F103C8) || defined(LMCU_STM32F103R8) || defined(LMCU_STM32F103T8) || \
    defined(LMCU_STM32F103V8) || defined(LMCU_STM32F103CB) || defined(LMCU_STM32F103RB) || \
    defined(LMCU_STM32F103TB) || defined(LMCU_STM32F103VB)
  #define LMCU_STM32F1_MEDIUM_DENSITY
#endif

// High-density devices are STM32F101xx and STM32F103xx microcontrollers where the Flash memory
// density ranges between 256 and 512 Kbytes.
#if defined(LMCU_STM32F101RC) || defined(LMCU_STM32F101VC) || defined(LMCU_STM32F101ZC) || \
    defined(LMCU_STM32F101RD) || defined(LMCU_STM32F101VD) || defined(LMCU_STM32F101ZD) || \
    defined(LMCU_STM32F101RE) || defined(LMCU_STM32F101VE) || defined(LMCU_STM32F101ZE) || \
    defined(LMCU_STM32F103RC) || defined(LMCU_STM32F103VC) || defined(LMCU_STM32F103ZC) || \
    defined(LMCU_STM32F103RD) || defined(LMCU_STM32F103VD) || defined(LMCU_STM32F103ZD) || \
    defined(LMCU_STM32F103RE) || defined(LMCU_STM32F103VE) || defined(LMCU_STM32F103ZE)
  #define LMCU_STM32F1_HIGH_DENSITY
#endif

// XL-density devices are STM32F101xx and STM32F103xx microcontrollers where the Flash memory
// density ranges between 768 Kbytes and 1 Mbyte.
#if defined(LMCU_STM32F101RF) || defined(LMCU_STM32F101VF) || defined(LMCU_STM32F101ZF) || \
    defined(LMCU_STM32F101RG) || defined(LMCU_STM32F101VG) || defined(LMCU_STM32F101ZG) || \
    defined(LMCU_STM32F103RF) || defined(LMCU_STM32F103VF) || defined(LMCU_STM32F103ZF) || \
    defined(LMCU_STM32F103RG) || defined(LMCU_STM32F103VG) || defined(LMCU_STM32F103ZG)
  #define LMCU_STM32F1_XL_DENSITY
#endif

// Connectivity line devices are STM32F105xx and STM32F107xx microcontrollers.
#if defined(LMCU_STM32F105) || defined(LMCU_STM32F107)
  #define LMCU_STM32F1_CONNECTIVITY
#endif

